#include <stdio.h>

int main() {
    int r, c, val;
    scanf("%d %d", &r, &c);
    switch (r % 2) {
        case 1:
            val = 10 * ((r - 1) / 2) + 2 * (c - 1);
            break;
        case 0:
            val = 10 * ((r - 2) / 2) + 1 + 2 * (c - 1);
            break;
    }
    printf("%d\n", val);
    return 0;
}

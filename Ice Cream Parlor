#include <stdio.h>
#include <stdlib.h>

int* icecreamParlor(int m, int cost[], int n) {
    int* result = (int*)malloc(2 * sizeof(int));
    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if (cost[i] + cost[j] == m) {
                result[0] = i + 1;
                result[1] = j + 1;
                return result;
            }
        }
    }
    return result;
}

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int m, n;
        scanf("%d %d", &m, &n);
        int* cost = (int*)malloc(n * sizeof(int));
        for (int i = 0; i < n; i++) {
            scanf("%d", &cost[i]);
        }
        int* result = icecreamParlor(m, cost, n);
        printf("%d %d\n", result[0], result[1]);
        free(cost);
        free(result);
    }
    return 0;
}
